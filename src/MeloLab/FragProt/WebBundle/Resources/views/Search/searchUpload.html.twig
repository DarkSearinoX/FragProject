{% extends 'MeloLabFragProtWebBundle:Search:base.html.twig' %}
{% form_theme form 'MeloLabFragProtWebBundle:Form:niceFields.html.twig' %}

{%block page_title%}Upload PDB Search{%endblock%}

{% block main %}


<img src="{{asset('bundles/melolabfragprotweb/images/clust-50.png')}}" height="50px" />
<form action="{{ path('fragprot_search_results') }}" method="post">
    <table>
        <tr><td>{{ form_label(form.fragmentPdb) }}</td></tr>
        <tr><td>{{ form_widget(form.fragmentPdb) }}</td></tr>
        <tr><td>{{ form_errors(form.fragmentPdb) }}</td></tr>
        <tr><td>{{ form_widget(form)}}</td></tr>
        <tr><td><button type="submit">Upload & Search</button></td></tr>
        
    </table>

    
    
</form>
 

{% endblock %}
